{
    "name": "evilfreelancer/openvpn-php",
    "description": "OpenVPN config generator writen on PHP",
    "keywords": [
        "openvpn",
        "config",
        "generate",
        "import",
        "laravel",
        "lumen",
        "plugin",
        "package"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "homepage": "http://drteam.rocks/",
            "email": "paul@drteam.rocks",
            "name": "Paul Rock",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "OpenVPN\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\OpenVPN\\": "./tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "ext-mbstring": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "larapack/dd": "^1.1",
        "friendsofphp/php-cs-fixer": "^2.16",
        "larapack/dd": "^1.1",
        "limedeck/phpunit-detailed-printer": "^5.0",
        "orchestra/testbench": "^4.0|^5.0"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "scripts": {
        "test:lint": "php-cs-fixer fix -v --dry-run",
        "test:unit": "phpunit",
        "test": [
            "@test:lint",
            "@test:unit"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenVPN\\Laravel\\ServiceProvider"
            ],
            "aliases": {
                "OpenVPN": "OpenVPN\\Laravel\\Facade"
            }
        }
    }
}
